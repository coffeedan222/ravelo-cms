{
  "name": "ravelo-cms",
  "version": "0.0.3",
  "description": "Ravelo Content Management System",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --inspect=0.0.0.0:3001 server.js",
    "test": "./node_modules/.bin/lab",
    "tdd": "./node_modules/.bin/nodemon ./node_modules/.bin/lab",
    "test:unit": "./node_modules/.bin/lab test/unit",
    "test:integration": "./node_modules/.bin/lab test/integration",
    "server:start": "./bin/start",
    "server:shutdown": "./bin/shutdown",
    "docs:build": "./bin/build",
    "frontend:build": "./node_modules/.bin/webpack --mode production",
    "frontend:start": "./node_modules/.bin/webpack-dev-server --open --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravelo-systematic-solutions/ravelo-cms.git"
  },
  "engine": {
    "npm": ">=6.1.0",
    "node": ">=10.1.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ravelo-systematic-solutions/ravelo-cms/issues"
  },
  "homepage": "https://github.com/ravelo-systematic-solutions/ravelo-cms#readme",
  "dependencies": {
    "hapi": "^17.5.1",
    "hapi-swagger": "^9.1.1",
    "inert": "^5.1.0",
    "mongoose": "^5.1.3",
    "vision": "^5.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "code": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lab": "^15.4.5",
    "nodemon": "^1.17.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
